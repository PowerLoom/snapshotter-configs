{
  "namespace": "relevant-namespace",
  "signer_private_key": "signer-account-private-key",
  "core_api": {
    "host": "0.0.0.0",
    "port": 8002,
    "auth": {
      "enabled": false,
      "header_key": "X-API-KEY"
    },
    "public_rate_limit": "20000/day;300/minute;10/second"
  },
  "instance_id": "account-address",
  "data_market": "data-market-contract",
  "slot_id": "slot-id",
  "async_task_config": {
    "task_timeout": 300,
    "task_cleanup_interval": 60
  },
  "local_collector_port": "local-collector-port",
  "rpc": {
    "full_nodes": [{
      "url": "https://rpc-url",
      "rate_limit": {
        "requests_per_second": "source-rpc-rate-limit"
      }
    }],
    "archive_nodes": [],
    "force_archive_blocks": 100,
    "retry": 5,
    "request_time_out": 5,
    "skip_epoch_threshold_blocks": 30,
    "polling_interval": 3,
    "semaphore_value": 20,
    "connection_limits":{
      "max_connections": 100,
      "max_keepalive_connections": 50,
      "keepalive_expiry": 300
    }
  },
  "rlimit": {
    "file_descriptors": 40960
  },
  "rabbitmq": {
    "user": "guest",
    "password": "guest",
    "host": "rabbitmq",
    "port": 5672,
    "setup": {
      "core": {
        "exchange": "powerloom-backend"
      },
      "callbacks": {
        "exchange": "powerloom-backend-callbacks"
      },
      "commit_payload": {
        "exchange": "powerloom-backend-commit-payload"
      },
      "event_detector": {
        "exchange": "powerloom-backend-event-detector"
      },
      "delegated_worker": {
        "exchange": "powerloom-backend-delegated-worker"
      }
    }
  },
  "reporting": {
    "slack_url": "https://slack-reporting-url",
    "service_url": "https://powerloom-reporting-url",
    "telegram_url": "https://telegram-reporting-url",
    "telegram_chat_id": "telegram-chat-id",
    "failure_report_frequency": 5
  },
  "redis": {
    "host": "redis-host",
    "port": "redis-port",
    "db": 0,
    "password": "redis-password",
    "ssl": false,
    "cluster_mode": false
  },
  "redis_reader": {
    "host": "redis-host",
    "port": "redis-port",
    "db": 0,
    "password": "redis-password"
  },
  "logs": {
    "trace_enabled": false,
    "write_to_files": true
  },
  "projects_config_path": "config/projects.json",
  "aggregator_config_path": "config/aggregator.json",
  "preloader_config_path": "config/preloader.json",
  "protocol_state": {
    "abi": "snapshotter/static/abis/ProtocolContract.json",
    "address": "protocol-state-contract",
    "deadline_buffer": 1000,
    "day_counter_buffer": 0
  },
  "callback_worker_config": {
    "num_delegate_workers": 0,
    "num_snapshot_workers": 4,
    "num_aggregation_workers": 6
  },
  "ipfs": {
    "url": "ipfs-writer-url",
    "url_auth": {
      "apiKey": "ipfs-writer-key",
      "apiSecret": "ipfs-writer-secret"
    },
    "reader_url": "ipfs-reader-url",
    "reader_url_auth": {
      "apiKey": "ipfs-reader-key",
      "apiSecret": "ipfs-reader-secret"
    },
    "write_rate_limit": {
      "req_per_sec": 10,
      "burst": 10
    },
    "timeout": 60,
    "connection_limits":{
      "max_connections": 100,
      "max_keepalive_connections": 50,
      "keepalive_expiry": 300
    },
    "remote_pinning": {
      "enabled": false,
      "service_name": "",
      "service_endpoint": "",
      "service_token": "",
      "background_pinning": false
    }
  },
  "web3storage":{
    "upload_snapshots": false,
    "upload_aggregates": true,
    "url": "https://api.web3.storage",
    "upload_url_suffix": "/upload",
    "api_token": "web3-storage-token",
    "timeout": 30,
    "max_idle_conns": 5,
    "idle_conn_timeout": 300
  },
  "anchor_chain_rpc": {
    "full_nodes": [{
      "url": "https://prost-rpc-url",
      "rate_limit": {
        "requests_per_second": "prost-rpc-rate-limit"
      }
    }],
    "retry": 5,
    "request_time_out": 5,
    "connection_limits":{
      "max_connections": 100,
      "max_keepalive_connections": 50,
      "keepalive_expiry": 300
    }
  },
  "httpx":{
    "pool_timeout": 10,
    "connect_timeout": 10,
    "read_timeout": 60,
    "write_timeout": 60
  },
  "node_version": "v1.2"
}
